{
  "name": "honey-noodles",
  "description": "Extension",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "honey-noodles",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "minimum_chrome_version": "10.0",
  "chrome_url_overrides": {
    "newtab": "page-new-tab/newtab.html"
  },
  "permissions": ["identity", "tabs", "webRequest"]
}
